{
  "name": "levelworks-ogp",
  "version": "8.12.0",
  "private": true,
  "dependencies": {
    "@aws-amplify/ui-react": "^4.2.1",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.3",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "aws-amplify": "^5.0.7",
    "axios": "^0.22.0",
    "clsx": "^1.1.1",
    "dotenv": "^16.0.0",
    "dotenv-cli": "^5.0.0",
    "file-saver": "^2.0.5",
    "lodash": "^4.17.21",
    "luxon": "^2.3.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hook-form": "^7.16.2",
    "react-multi-date-picker": "^3.1.7",
    "react-query": "^3.24.5",
    "react-responsive-carousel": "^3.2.23",
    "react-router-dom": "^5.3.0"
  },
  "scripts": {
    "start": "vite",
    "build:test": "dotenv -e ./.env.ogp.test.levelworks.dev vite build",
    "build:acc": "dotenv -e ./.env.ogp.acc.levelworks.dev vite build",
    "build:prod": "dotenv -e ./.env.ogp.prod.levelworks.dev vite build",
    "check": "npm run check:typescript && npm run check:prettier && npm run check:eslint",
    "check:typescript": "tsc -p . --noEmit",
    "check:prettier": "prettier --check \"./src/**/*.{ts,tsx}\"",
    "check:eslint": "eslint src --cache-location=node_modules/.cache/eslint --ext .js,.jsx,.ts,.tsx --max-warnings 0",
    "fix:eslint": "npm run check:eslint -- --fix",
    "format": "npm run check:prettier -- --write",
    "test": "vitest",
    "test:e2e": "cypress run -C cypress/cypress.config.ts",
    "test:e2e:ui": "cypress run -C cypress/cypress.config.ts --headed --browser chrome"
  },
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^13.0.2",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.1.5",
    "@testing-library/dom": "^8.17.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^11.2.7",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.4.3",
    "@types/file-saver": "^2.0.3",
    "@types/jest": "^26.0.24",
    "@types/lodash": "^4.14.175",
    "@types/luxon": "^2.3.1",
    "@types/qs": "^6.9.7",
    "@types/react": "^17.0.26",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.3.0",
    "@types/react-test-renderer": "^17.0.2",
    "@vitejs/plugin-react": "^3.0.0",
    "cypress": "^10.10.0",
    "eslint-plugin-jest": "^27.1.6",
    "msw": "^0.49.2",
    "prettier": "^2.4.1",
    "react-test-renderer": "^17.0.2",
    "typescript": "^4.9.4",
    "vite": "^4.0.1",
    "vite-plugin-svgr": "^2.4.0",
    "vite-tsconfig-paths": "^4.0.3",
    "vitest": "^0.26.0"
  }
}
